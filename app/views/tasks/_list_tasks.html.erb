<div class="col-md-12">
  <h2>Tasks</h2>
</div>

<div class="col-md-6">
  <strong>Incomplete</strong>
  <% not_completed_tasks.each do |task| %>
    <%= render 'task', task: task %>
  <% end %>
</div>

<div class="col-md-6">
  <strong>Complete</strong>
  <% completed_tasks.each do |task| %>
    <%= render 'task', task: task %>
  <% end %>
</div>
